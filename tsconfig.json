{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM",
      "DOM.iterable"
    ],
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": false,
    "experimentalDecorators": true,
    "rootDir": "./src/assets/typescript",
    "outDir": "./dist/assets/javascript",
    "types": [
      "@types/node"
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt"
  ]
}
